<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="CyberdropDownloader.Avalonia.Views.AlbumTitle">

  <Label x:Name="AlbumTitleLabel"
         Content="Downloading: None"
         Foreground="{DynamicResource AlbumTitleLabelForeground}"
         FontSize="12" FontFamily="Microsoft Sans Serif" FontWeight="Thin"
         HorizontalAlignment="Left"
         Padding="0"/>
  
</UserControl>
